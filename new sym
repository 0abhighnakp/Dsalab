#include<stdio.h>

//to read matrix
void sparce(int sp[100][3])
{
    int i,j,k=1;
    printf("Enter total no. of rows of sparse matrix :");
    scanf("%d",&sp[0][0]);
    printf("Enter total no. of cols of sparse matrix :");
    scanf("%d",&sp[0][1]);
    printf("Enter total no. of non zero elements:");
    scanf("%d",&sp[0][2]);
            for(i=1;i<sp[0][2]+1;i++){
                printf("Enter row,col,value :");
                for(j=0;j<3;j++){
                       scanf("%d",&sp[i][j]);
                }
            }
        for(k=0;k<sp[0][2]+1;k++){
            for(j=0;j<3;j++){
                printf("%d",sp[k][j]);
            }
            printf("\n");
        }
}
void transpose(int mat_trp[100][3])
{
    
    int temp[100],i,j;
    int tr=mat_trp[0][2];
    for(i=0;i<tr;i++)
    {
        temp[i]=mat_trp[i][0];
    }
    for(i=0;i<tr;i++)
    {
        mat_trp[i][0]= mat_trp[i][1];
         mat_trp[i][1]=temp[i];
    }
    
    printf("\nThe Transpose is\n");
    for(i=0;i<tr+1;i++)
    {
        printf("\n");
        for(j=0;j<3;j++)
        {
            printf("%d  ",mat_trp[i][j]);
        }
    }
    
}
void Symmetric (int a[100][3],int b[100][3]){
    int i,j,flag=0;
    for(i=0;i<a[0][3];i++){
        for(j=0;j<3;j++){
            if(a[i][j]!=b[i][j]){
                flag=1;
                break;
            }
        }
    }
    if(flag==0){
       printf("Symmetric");
    }
    else{
        printf("Not Symmetric");
    }
}


void main()
{
    int mat[100][100];
    int mat2[100][100];
    sparce(mat);
    transpose(mat);
    Symmetric(mat, mat2);
}

